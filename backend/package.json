{
  "name": "lumberjack",
  "version": "1.0.0",
  "description": "Backend projject for order management of a lumber yard company based on NestJS framework.",
  "author": "D치niel Papilion and B치lint L치szl칩",
  "license": "MIT",
  "engines": {
    "node": "10.16.0",
    "npm": "6.14.3"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:prod": "npm run prebuild && npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prestart": "typeorm migration:run",
    "start": "nest start",
    "start:dev": "npm run start -- --watch",
    "start:debug": "npm run start -- --watch --debug",
    "start:prod": "node dist/src/main.js"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.11",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^6.11.11",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/swagger": "^4.4.0",
    "@nestjs/typeorm": "^6.3.4",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.1",
    "helmet": "^3.22.0",
    "mariadb": "^2.2.0",
    "mysql": "^2.18.1",
    "nestjs-rate-limiter": "^1.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.1",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^6.11.11",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.3",
    "@types/helmet": "0.0.45",
    "@types/jest": "25.1.2",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^25.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/papiliond/lumberjack"
  }
}
